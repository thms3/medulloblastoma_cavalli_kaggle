def gen_nodes_in_path(path:list):

        for i in path:
            for j in path:
                if i!=j:
                    yield i,j

def mask_adj_mat(path:list,adj_mat:np.ndarray=None):

    mask = np.zeros_like(adj_mat)

    
    for i,j in gen_nodes_in_path(path):
        mask[i,j]+=1

    mask_adj_mat = adj_mat*mask

    return mask_adj_mat


